@model IEnumerable<AddOrRemoveUsersinRoleVM>

<h1>Add or Remove Users in this role</h1>


<form method="post" action="AddOrRemoveUsersInRole">

    <div class="form-group">    </div>
    @if (Model.Any())
    {
        foreach (var user in Model)
        {
            <div class="form-check">
                <input class="form-check-input" type="checkbox" asp-for="@user.IsSelected">
                <label class="form-check-label" asp-for="@user.IsSelected">
                   @user.UserName
                </label>
                </div>
                }
                }
    <input type="hidden" />


        </form>    